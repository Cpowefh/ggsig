{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ],
  {
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/static/dashboard.html"  # 或你的实际文件路径
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
}
